<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="testSubTree">
    <Sequence>
      <AlwaysSuccess name="A"/>
      <AlwaysSuccess/>
      <Sleep msec="1000"/>
      <SetPose x="0.28"
               y="0.4"
               z="0.5"
               frame_id="test"
               pose_stamped="{pose}"/>
      <PrintPose pose_stamped="{pose}"/>
      <Parallel failure_count="1"
                success_count="-1">
        <PlanToPose pose_stamped="{pose}"
                    planing_group="panda_arm"
                    plan="{plan}"/>
        <PlanToPose pose_stamped="{pose}"
                    planing_group="panda_arm"
                    plan="{plan}"/>
      </Parallel>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="PlanToPose"
            editable="true">
      <input_port name="pose_stamped"/>
      <input_port name="planing_group"/>
      <output_port name="plan"/>
    </Action>
    <Action ID="PrintPose"
            editable="true">
      <input_port name="pose_stamped"/>
    </Action>
    <Action ID="SetPose"
            editable="true">
      <input_port name="x"/>
      <input_port name="y"/>
      <input_port name="z"/>
      <input_port name="frame_id"/>
      <output_port name="pose_stamped"/>
    </Action>
  </TreeNodesModel>

</root>
