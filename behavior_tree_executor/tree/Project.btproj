<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4" project_name="Project">
    <include path="main.xml"/>
    <include path="subtree.xml"/>
    <!-- Description of Node Models (used by Groot) -->
    <TreeNodesModel>
        <Action ID="ExecutePlan" editable="true">
            <input_port name="trajectory"/>
            <input_port name="planing_group"/>
        </Action>
        <Action ID="MoveToPose" editable="true">
            <input_port name="pose_stamped"/>
            <input_port name="planing_group"/>
        </Action>
        <Action ID="PlanToPose" editable="true">
            <input_port name="pose_stamped"/>
            <input_port name="planing_group"/>
            <output_port name="trajectory"/>
        </Action>
        <Action ID="PlanToPoseCartesian" editable="true">
            <input_port name="pose_stamped"/>
            <input_port name="planing_group"/>
            <output_port name="trajectory"/>
        </Action>
        <Action ID="PrintPose" editable="true">
            <input_port name="pose_stamped"/>
        </Action>
        <Action ID="ReceiveString" editable="true"/>
        <Action ID="SetPose" editable="true">
            <input_port name="x"/>
            <input_port name="y"/>
            <input_port name="z"/>
            <input_port name="frame_id"/>
            <output_port name="pose_stamped"/>
        </Action>
    </TreeNodesModel>
</root>
